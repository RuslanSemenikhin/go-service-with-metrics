<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{{ .Title }}</title>
</head>
<body>
    <h1>{{ .Header }}</h1>
    <h3>{{ .Counter }}</h3>
        {{ range $key, $value := .CountersMetrics }}
            <li>{{ $key }}: {{ $value }}</li>
        {{ end }}
    <h3>{{ .Gauge }}</h3>
        {{ range $key, $value := .GaugesMetrics }}
            <li>{{ $key }}: {{ $value }}</li>
        {{ end }}
</body>
</html>